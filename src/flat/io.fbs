namespace io;

table Command {
    player_id: uint32;
    room_id: uint32;
    move_x: int8;
    move_y: int8;
    attack: bool;
    skill1: bool;
    skill2: bool;
}


table Entity {
    entity_id: uint32;
    hp: int32;
    status_flags: uint32;
    x: float;
    y: float;
    vx: float;
    vy: float;
}

enum DataType : byte {
    None = 0,
    Entitys = 1,
    Commands = 2
}

table Frame {
    tick: uint32;
    type: DataType;
    entities: [Entity];
    commands: [Command];
}